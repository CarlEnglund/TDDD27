<%= react_component('Header', text: 'Moviepalace') %>
  <% @movie.each do |movie| %>
      <tr>
        <td style="width:15%">
          <%= link_to movie_path(movie.id) do %>
              <%= image_tag("#{@configuration.base_url}w154#{movie.poster_path}") if movie.poster_path %>
          <% end %>
        </td>
        <td>
          <%= link_to movie.title, movie_path(movie.id) %> <br />


        </td>
      </tr>
  <% end %>
</table>

<%= react_component('Footer') %>